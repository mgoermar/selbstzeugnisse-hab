    <text>
        <body>
            <div><head facs="http://selbstzeugnisse.hab.de/edition/images/00101.jpg" n="49r">Anno 1622</head></div>
            
            <div xml:id="E1622-02-16"><head><date when="1622-02-16">den 16 Febr<ex>uari</ex> <c>♄</c>,</date></head><p> biß <rs ref="register.xml#Langwedel" type="place">Langwedel</rs>. 4 m<ex>eill</ex></p></div>
            
            <div xml:id="E1622-02-17"><head><date when="1622-02-17">d<ex>e</ex>n 17 Febr<ex>uari</ex> <c>☉</c>.</date></head><p> biß <rs ref="register.xml#Walsrode" type="place">Walßrade</rs>, 3.
                m<ex>eill</ex>.</p></div>
            
            <div xml:id="E1622-02-18"><head><date when="1622-02-18">d<ex>e</ex>n 18. Febr<ex>uari</ex>
                <c>☽</c>.</date></head><p> biß <rs ref="register.xml#Oerrel" type="place">Orle</rs>, <rs ref="register.xml#Ebstorf" type="place">Ebstorf</rs>,</p></div>
            
            <div xml:id="E1622-02-19"><head><date when="1622-02-19">d<ex>e</ex>n 19 Febr<ex>uari</ex>
                <c>♂</c></date></head><p> biß <rs ref="register.xml#Hitzacker" type="place">Hitzger</rs>.</p></div>
            
            <pb facs="http://selbstzeugnisse.hab.de/edition/images/00102.jpg" n="49v"/>
            
            <div><head><foreign xml:lang="lat"><p>Eodem Anno.</p></foreign><note type="translation">Im selben Jahr</note></head></div>
            
            <div xml:id="E1622-07-10"><head><date when="1622-07-10">den 10. Julÿ. <c>☿</c>.</date></head><p> biß <rs ref="register.xml#Neustadt_Glewe" type="place">Vergün:
                Newstadt</rs>, 5. m<ex>eill</ex>.</p></div>
            
            <div xml:id="E1622-07-11"><head><date when="1622-07-11">den 11. Julÿ. <c>♃</c></date></head><p> biß <rs ref="register.xml#Plau" type="place">Plawe</rs>.</p></div>
            
            <div xml:id="E1622-07-12"><head><date when="1622-07-12">den 12 Julÿ, <c>♀</c>,</date></head><p> biß <rs ref="register.xml#Mirow" type="place">Mirow</rs>.</p></div>
            
            <div xml:id="E1622-07-13"><head><date when="1622-07-13">d<ex>e</ex>n 13 Julÿ. <c>♄</c></date></head><p> biß <rs ref="register.xml#Neubrandenburg" type="place">N<ex>eu</ex> Brandenburgk</rs>.</p></div>
            
            <div xml:id="E1622-07-14"><head><date when="1622-07-14">den 14. Julÿ, <c>☉</c></date></head><p> biß <rs ref="register.xml#Torgelow" type="place">Torgerlaw.</rs></p></div>
            
            <div xml:id="E1622-07-15"><head><date when="1622-07-15">den 15 Julÿ. <c>☽</c>.</date></head><p> biß <rs ref="register.xml#Loecknitz" type="place">Löckenitz</rs>;
                <rs ref="register.xml#Stettin" type="place">Alten Stetin</rs>:</p></div>
            
            <div xml:id="E1622-10-12"><head><date when="1622-10-12">den 12 8bris. <c>♄</c></date></head><p> wied<ex>er</ex> zu rücke, biß <rs ref="register.xml#Torgelow" type="place">Torgerlaw</rs>.</p></div>
            
            <div xml:id="E1622-10-13"><head><date when="1622-10-13">den 13 8bris <c>☉</c>.</date></head><p> biß <rs ref="register.xml#Neubrandenburg" type="place">N<ex>eu</ex> Brandenburgk.</rs></p></div>
            
            <div xml:id="E1622-10-14"><head><date when="1622-10-14">den 14. 8bris, <c>☽</c>.</date></head><p> biß <rs ref="register.xml#Mirow" type="place">Miraw</rs>.</p></div>
            
            <div xml:id="E1622-10-15"><head><date when="1622-10-15">den 15. 8bris <c>♂</c>.</date></head><p> Stille
                gelegen.</p></div>
            
            <div xml:id="E1622-10-16"><head><date when="1622-10-16">den 16. 8bris. <c>☿</c></date></head><p> 
                biß <subst><del rend="overstrike">N<ex>eu</ex> Brandenburgk</del> <add place="above"><rs ref="register.xml#Plau" type="place">Plawe</rs>.</add></subst></p></div>
            
            <div xml:id="E1622-10-17"><head><date when="1622-10-17">den 17. 8bris <c>♃</c>.</date></head><p> biß <rs ref="register.xml#Grabow" type="place">Grabow</rs>.</p></div>
            
            <div xml:id="E1622-10-19"><head><date when="1622-10-19">d<ex>e</ex>n 19. 8bris <c>♄</c>.</date></head><p> biß <rs ref="register.xml#Hitzacker" type="place">Hitzger</rs>.</p></div>
            
            <div><head><p><foreign xml:lang="lat">Eodem Anno.</foreign><note type="translation">Im selben Jahr</note></p></head></div>
            
            <div xml:id="E1622-12-16"><head><date when="1622-12-16">d<ex>e</ex>n 16 Xbris <c>☽</c>.</date></head><p> biß <rs ref="register.xml#Scharnebeck" type="place">Scharnebeck</rs>.</p></div>
            
            <div xml:id="E1622-12-17"><head><date when="1622-12-17">den 17. Xbris <c>♂</c></date></head><p> biß
                <rs ref="register.xml#Schneverdingen" type="place">Schneverding</rs>.</p></div>
            
            <div xml:id="E1622-12-18"><head><date when="1622-12-18">d<ex>e</ex>n 18 Xbris <c>☿</c></date></head><p> biß <rs ref="register.xml#Langwedel" type="place">Langwedel</rs>.</p></div>
            
            <div xml:id="E1622-12-19"><head><date when="1622-12-19">d<ex>e</ex>n 19. Xbris <c>♃</c>,</date></head><p> biß <rs ref="register.xml#Delmenhorst" type="place">Delmenhorst</rs>.</p></div>
            
            <div xml:id="E1622-12-26"><head><date when="1622-12-26">den 26 Xbris <c>♃</c>.</date></head><p> biß
                <rs ref="register.xml#Langwedel" type="place">Langwedel</rs>.</p></div>
            
            <div xml:id="E1622-12-27"><head><date when="1622-12-27">d<ex>e</ex>n 27. Xbris <c>♀</c></date></head><p> biß <rs ref="register.xml#Schneverdingen" type="place">Schneverding</rs>.</p></div>
            
            <div xml:id="E1622-12-28"><head><date when="1622-12-28">d<ex>e</ex>n 28. Xbris <c>♄</c>,</date></head><p> biß <rs ref="register.xml#Scharnebeck" type="place">Scharnebeck</rs>.</p></div>
           
            <div xml:id="E1622-12-29"><head><date when="1622-12-29">d<ex>e</ex>n 29. Xbris <c>☉</c></date></head><p> biß <rs ref="register.xml#Hitzacker" type="place">Hitzger</rs>.</p></div>
            
